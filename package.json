{
  "name": "deepseek-assistant",
  "version": "1.0.0",
  "description": "Interface de chat para DeepSeek API",
  "type": "module",
  "scripts": {
    "dev:frontend": "vite",
    "dev:backend": "ts-node-dev --respawn --transpile-only --project .\\backend\\tsconfig.json .\\backend\\src\\server.ts",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "windows:dev": "powershell -ExecutionPolicy Bypass -File .\\config\\startup.ps1"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@types/uuid": "^10.0.0",
    "axios": "^1.9.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "html2canvas": "^1.4.1",
    "i18next": "^25.1.3",
    "i18next-browser-languagedetector": "^8.1.0",
    "lucide-react": "^0.511.0",
    "react": "^18.3.1",
    "react-calendar": "^5.1.0",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.5.1",
    "react-icons": "^4.12.0",
    "react-router-dom": "^7.5.3",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/html2canvas": "^0.5.35",
    "@types/node": "^20.11.28",
    "@types/react": "^18.3.20",
    "@types/react-dom": "^18.3.6",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "vite": "^5.4.17"
  }
}
